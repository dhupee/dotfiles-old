{
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vim.smartRelativeLine": true,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "workbench.colorTheme": "Dracula Theme",
    "workbench.iconTheme": "simple-icons",
    "editor.formatOnSave": true,
    "editor.quickSuggestionsDelay": 5,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on"
    },
    "python.defaultInterpreterPath": "/home/dhupee/.pyenv/versions/3.10.14/envs/aibilytiqs",
    "ruff.interpreter": [
        "/home/dhupee/.pyenv/versions/3.12.1/bin/python"
    ],
    "python.languageServer": "Jedi",
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "clangd.path": "/home/dhupee/.config/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/18.1.3/clangd_18.1.3/bin/clangd",
    "hediet.vscode-drawio.resizeImages": null,
    "files.autoSave": "afterDelay",
    "hediet.vscode-drawio.theme": "dark",
    "hediet.vscode-drawio.codeLinkActivated": true,
    "explorer.confirmDragAndDrop": false,
    "editor.lineNumbers": "relative"
}